{
  "name": "element-plus-formkit",
  "version": "0.1.6",
  "main": "dist/formkit.umd.js",
  "module": "dist/formkit.es.js",
  "types": "types/element-plus-formkit.d.ts",
  "style": "dist/index.css",
  "description": "Data-driven form component based on ElementPlus implementation",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "dev": "node scripts/start-dev.mjs",
    "watch": "vite build --watch",
    "build": "vite build --mode development && vue-tsc --noEmit",
    "build:production": "vite build --mode production && vue-tsc --noEmit",
    "lint:eslint": "eslint  --fix --ext .ts,.js,.vue ./src ",
    "lint:prettier": "prettier --write \"**/*.{js,cjs,ts,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint  \"**/*.{css,scss,vue}\" --fix"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{cjs,json}": [
      "prettier --write"
    ],
    "*.{vue,html}": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{scss,css}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "type": "module",
  "peerDependencies": {
    "element-plus": "^2.0.0",
    "vue": "^3.0.0"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vueuse/core": "^10.1.2",
    "element-plus": "^2.3.14",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@iconify-json/ep": "^1.1.10",
    "@types/stompjs": "^2.3.5",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "autoprefixer": "^10.4.19",
    "chalk": "^5.4.1",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.13.0",
    "fs-extra": "^11.3.0",
    "lint-staged": "^13.2.2",
    "ora": "^8.2.0",
    "postcss": "^8.4.38",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.8",
    "sass": "^1.58.3",
    "stylelint": "^15.10.2",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-recommended-scss": "^12.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "typescript": "^5.7.2",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.4.11",
    "vue-tsc": "^2.1.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wxsub/element-plus-formkit.git"
  },
  "homepage": "https://wxsub.github.io/element-plus-formkit",
  "keywords": [
    "typescript",
    "vue",
    "vue3",
    "formkit",
    "el-form",
    "element-plus"
  ],
  "author": "wxsub.com studios Â© 2023",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
